package br.com.ctb.executorsql.exception;

public class ControladorException extends Exception {

	/**
	 * 
	 */
	private static final long serialVersionUID = 417567242611841679L;
	
	public ControladorException(String message) {
		super(message);
	}
	
	public ControladorException(String message, Exception e) {
		super(message, e);
	}
	
	public ControladorException(Exception e) {
		super(e);
	}
	
}
